{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-airbnb",
        "tslint-config-prettier",
        "tslint-plugin-prettier"
    ],
    "jsRules": {},
    "rules": {
        "import-name": false,
        "no-boolean-literal-compare": false,
        "ter-indent": ["error", "spaces", 4],
        "prettier": [true, "./prettierrc"]
    },
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ]
}